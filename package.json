{
  "name": "@jaggerxtrm/unified-ai-mcp-tool",
  "version": "1.2.1",
  "description": "Unified Model Context Protocol server for multiple AI clients (Qwen, Rovo Dev, Gemini etc.)",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "unified-ai-mcp-tool": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/index.js",
    "lint": "tsc --noEmit",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "view-metrics": "tsc && node dist/scripts/view-token-metrics.js",
    "monitor-agents": "tsx scripts/monitor-agent-performance.ts"
  },
  "keywords": [
    "mcp",
    "qwen",
    "rovodev",
    "ai",
    "model-context-protocol",
    "qwen-code",
    "acli",
    "rovodev-cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fission-ai/openspec": "^0.15.0",
    "@modelcontextprotocol/sdk": "^1.21.0",
    "better-sqlite3": "^11.8.1",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^24.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
